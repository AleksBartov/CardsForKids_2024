{
  "expo": {
    "name": "Учу слоги",
    "slug": "cards-for-kids",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#01569E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aleksbartov.cardsforkids"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appIconForAndroid.png",
        "backgroundColor": "#01569E"
      },
      "package": "com.aleksbartov.cardsforkids",
      "versionCode": 6
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
